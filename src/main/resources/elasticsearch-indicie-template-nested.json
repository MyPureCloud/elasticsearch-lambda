{
	"template": "c*",
	"settings": {
		"number_of_shards": 5,
		"number_of_replicas": 1,
		"index.query.default_field": "@id",
		"analysis": {
			"filter": {
				"email": {
					"type": "pattern_capture",
					"preserve_original": 1,
					"patterns": [
						"([^@]+)",
						"(\\p{L}+)",
						"(\\d+)",
						"@(.+)"
					]
				}
			},
			"analyzer": {
				"email": {
					"type": "custom",
					"tokenizer": "uax_url_email",
					"filter": [
						"email",
						"lowercase",
						"unique"
					]
				}
			}
		}
	},
	"mappings": {
        "_default_": {
            "_all": {
                "enabled": false
            },
            "_source": {
                "enabled": true
            },
            "dynamic_templates": [
                {
                    "string_exact": {
                        "match": "string_exact_*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "type": "string",
                            "index": "not_analyzed"
                        }
                    }
                },
                {
                    "string_fuzzy": {
                        "match": "string_fuzzy_*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "index": "analyzed",
                            "analyzer": "standard",
                            "type": "string"
                        }
                    }
                },
                {
                    "string_ignore_case": {
                        "match": "string_ignore_case_*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "index": "analyzed",
                            "analyzer": "standard",
                            "type": "string"
                        }
                    }
                },
                {
                    "uuid_template": {
                        "match": "uuid_*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "index": "not_analyzed",
                            "type": "string"
                        }
                    }
                },
                {
                    "date_template": {
                        "match": "date_*",
                        "mapping": {
                            "type": "date",
                            "dynamic_date_formats": [
                                "yyyy-MM-dd'T'HH:mm:ss.SSSZ"
                            ],
                            "format": "yyyy-MM-dd'T'HH:mm:ss.SSSZ"
                        },
                        "match_mapping_type": "date"
                    }
                },
                {
                    "long_template": {
                        "match": "long_*",
                        "mapping": {
                            "type": "long"
                        },
                        "match_mapping_type": "long"
                    }
                },
                {
                    "boolean_template": {
                        "match": "boolean_*",
                        "mapping": {
                            "type": "boolean"
                        },
                        "match_mapping_type": "boolean"
                    }
                },
                {
                    "double_template": {
                        "match": "double_*",
                        "mapping": {
                            "type": "double"
                        },
                        "match_mapping_type": "double"
                    }
                }
            ],
            "properties": {
                "fields": {
                    "type": "object",
                    "dynamic": true
                },
                "conversationId": {
                    "type": "text",
                    "index": false
                },
